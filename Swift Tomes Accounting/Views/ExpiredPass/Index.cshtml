@model IEnumerable<Swift_Tomes_Accounting.Models.ViewModels.ApplicationUser>


<br />
<div>
    <div class="col-6">
        <h2 class="text-primary">Expired Passwords List</h2>

    </div>
</div>

<div class="p-4 border rounded">
    @if (Model.Any())
    {
        <table class="table table-striped border">
            <tr class="table-secondary">

                <th>
                    Email
                </th>
                <th>
                    Name
                </th>
                <th>
                    Expired Hashed Password 1
                </th>


                <th></th>
            </tr>
            @foreach (var user in Model)
            {
        <tr>
            <td>
                @user.Email
            </td>
            <td>
                @user.FirstName  @user.LastName
            </td>
            <td>
                @user.LastPass1
            </td>


        </tr>
            }
        </table>
    }
</div>

<br />
<br />
<br />
<form asp-action="GoBack" asp-controller="Admin">
    <div class="container">
        <div class="row">
            <div class="col text-center">
                <button type="submit" class="btn btn-warning">Done</button>
            </div>
        </div>
    </div>
</form>






